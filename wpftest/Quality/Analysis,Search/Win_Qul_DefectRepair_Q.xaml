                                                    <TextBlock Grid.Row="6" Text="{Binding RepairRate12}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                </Grid>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>
                                    <DataGridTemplateColumn x:Name="dgdtpeAvgMonth" Header="일평균" >
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="auto"/>
                                                        <RowDefinition Height="auto"/>
                                                        <RowDefinition Height="auto"/>
                                                        <RowDefinition Height="auto"/>
                                                        <RowDefinition Height="auto"/>
                                                        <RowDefinition Height="auto"/>
                                                        <RowDefinition Height="auto"/>
                                                    </Grid.RowDefinitions>
                                                    <TextBlock Grid.Row="0" Text="{Binding AvgProdQty}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                    <Line Grid.Row="1" Stroke="Black" StrokeThickness="2" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                    <TextBlock Grid.Row="2" Text="{Binding AvgDefectQty}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                    <Line Grid.Row="3" Stroke="Black" StrokeThickness="1" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                    <TextBlock Grid.Row="4" Text="{Binding AvgRepairQty}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                    <Line Grid.Row="5" Stroke="Black" StrokeThickness="1" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                    <TextBlock Grid.Row="6" Text="{Binding AvgRepairRate}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                </Grid>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>
                                    <DataGridTemplateColumn x:Name="dgdtpeSumMonth" Header="합계" >
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="auto"/>
                                                        <RowDefinition Height="auto"/>
                                                        <RowDefinition Height="auto"/>
                                                        <RowDefinition Height="auto"/>
                                                        <RowDefinition Height="auto"/>
                                                        <RowDefinition Height="auto"/>
                                                        <RowDefinition Height="auto"/>
                                                    </Grid.RowDefinitions>
                                                    <TextBlock Grid.Row="0" Text="{Binding TProdQty}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                    <Line Grid.Row="1" Stroke="Black" StrokeThickness="2" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                    <TextBlock Grid.Row="2" Text="{Binding TDefectQty}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                    <Line Grid.Row="3" Stroke="Black" StrokeThickness="1" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                    <TextBlock Grid.Row="4" Text="{Binding TRepairQty}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                    <Line Grid.Row="5" Stroke="Black" StrokeThickness="1" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                    <TextBlock Grid.Row="6" Text="{Binding TRepairRate}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                </Grid>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>
                                </DataGrid.Columns>
                            </DataGrid>
                        </Grid>
                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0.12*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <StackPanel Grid.Row="0" Grid.Column="0" VerticalAlignment="Center" Orientation="Horizontal">
                                <TextBlock Text="●" Foreground="Red"/>
                                <TextBlock x:Name="tbkLabels1" Grid.Row="0" Grid.Column="0" Text="불량유형별" Margin="1"/>
                            </StackPanel>

                            <DataGrid x:Name="dgdMonthDefectSymptom" Grid.Row="1" Grid.Column="0" Style="{DynamicResource DataGridSet1}">
                                <DataGrid.RowStyle>
                                    <Style TargetType="DataGridRow">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Path=ColorBlue}" Value="true">
                                                <Setter Property="Background" Value="#365fb1" />
                                                <Setter Property="Foreground" Value="White" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </DataGrid.RowStyle>
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="" Binding="{Binding Num}"/>
                                    <DataGridTextColumn Header="월" Binding="{Binding InspectMonth}"/>
                                    <DataGridTextColumn Header="유형" Binding="{Binding GroupingName}"/>
                                    <DataGridTextColumn Header="불량수" Binding="{Binding DefectQty}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="시정수" Binding="{Binding RepairQty}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="시정률(%)" Binding="{Binding RepairRate}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                </DataGrid.Columns>
                            </DataGrid>

                            <StackPanel Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" Orientation="Horizontal">
                                <TextBlock Text="●" Foreground="Orange"/>
                                <TextBlock x:Name="tbkLabels2" Grid.Row="0" Grid.Column="0" Text="불량원인별" Margin="1"/>
                            </StackPanel>
                            <DataGrid x:Name="dgdMonthDefectReason" Grid.Row="1" Grid.Column="1" Style="{DynamicResource DataGridSet1}" >
                                <DataGrid.RowStyle>
                                    <Style TargetType="DataGridRow">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Path=ColorBlue}" Value="true">
                                                <Setter Property="Background" Value="#365fb1" />
                                                <Setter Property="Foreground" Value="White" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </DataGrid.RowStyle>
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="" Binding="{Binding Num}"/>
                                    <DataGridTextColumn Header="월" Binding="{Binding InspectMonth}"/>
                                    <DataGridTextColumn Header="원인" Binding="{Binding GroupingName}"/>
                                    <DataGridTextColumn Header="불량수" Binding="{Binding DefectQty}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="시정수" Binding="{Binding RepairQty}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="시정률(%)" Binding="{Binding RepairRate}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                </DataGrid.Columns>
                            </DataGrid>

                            <StackPanel Grid.Row="0" Grid.Column="2" VerticalAlignment="Center" Orientation="Horizontal">
                                <TextBlock Text="●" Foreground="LightGreen"/>
                                <TextBlock x:Name="tbkLabels3" Grid.Row="0" Grid.Column="0" Text="업체별" Margin="1"/>
                            </StackPanel>
                            <DataGrid x:Name="dgdMonthDefectCustom" Grid.Row="1" Grid.Column="2" Style="{DynamicResource DataGridSet1}">
                                <DataGrid.RowStyle>
                                    <Style TargetType="DataGridRow">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Path=ColorBlue}" Value="true">
                                                <Setter Property="Background" Value="#365fb1" />
                                                <Setter Property="Foreground" Value="White" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </DataGrid.RowStyle>
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="" Binding="{Binding Num}"/>
                                    <DataGridTextColumn Header="월" Binding="{Binding InspectMonth}"/>
                                    <DataGridTextColumn Header="업체명" Binding="{Binding GroupingName}"/>
                                    <DataGridTextColumn Header="불량수" Binding="{Binding DefectQty}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="시정수" Binding="{Binding RepairQty}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="시정률(%)" Binding="{Binding RepairRate}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                </DataGrid.Columns>
                            </DataGrid>
                        </Grid>
                        <DataGrid x:Name="dgdTotal3" Grid.Row="2" Grid.ColumnSpan="6"  Style="{DynamicResource DataGridSet1}" >
                            <DataGrid.Columns>
                                <DataGridTextColumn Header="" Binding="{Binding GroupingName}" CellStyle="{StaticResource DataGridCellAndCentering}" Width="50"/>
                                <DataGridTextColumn Header="불량수" Binding="{Binding DefectQty}" CellStyle="{StaticResource NumericDataGridCell}" Width="*"/>
                                <DataGridTextColumn Header="시정수" Binding="{Binding RepairQty}" CellStyle="{StaticResource NumericDataGridCell}" Width="*"/>
                                <DataGridTextColumn Header="시정률(%)" Binding="{Binding RepairRate}" CellStyle="{StaticResource NumericDataGridCell}" Width="*"/>
                            </DataGrid.Columns>
                        </DataGrid>
                    </Grid>
                </TabItem>
            </TabControl>
        </Grid>
    </Grid>
</UserControl>
