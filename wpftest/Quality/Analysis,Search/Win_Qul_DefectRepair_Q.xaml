<UserControl x:Class="WizMes_ANT.Win_Qul_DefectRepair_Q"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WizMes_ANT"
             mc:Ignorable="d" Loaded="UserControl_Loaded"
             xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             d:DesignHeight="620" d:DesignWidth="820">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="44*" />
            <RowDefinition Height="259*" />
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="{DynamicResource WizMes_ANT}">
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="100*" />
                <ColumnDefinition Width="160*" />
                <ColumnDefinition Width="180*" />
                <ColumnDefinition Width="180*" />
                <ColumnDefinition Width="200*" />
            </Grid.ColumnDefinitions>

            <Grid Grid.Column="1" Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="65*" />
                    <ColumnDefinition Width="85*" />
                </Grid.ColumnDefinitions>
                <Label x:Name="lblDate" Grid.Column="0" >
                    <StackPanel Orientation="Horizontal" Margin="-2">
                        <Viewbox>
                            <CheckBox x:Name="chkDate" IsChecked="True"/>
                        </Viewbox>
                        <TextBlock Text=" 일 자" VerticalAlignment="Center" Margin="-5, 0, 0, 0"/>
                    </StackPanel>
                </Label>
                <DatePicker x:Name="dtpSDate" Grid.Column="1" Padding="0" />
            </Grid>
            <Grid Grid.Column="1" Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="65*" />
                    <ColumnDefinition Width="85*" />
                </Grid.ColumnDefinitions>
                <DatePicker x:Name="dtpEDate" Grid.Column="1" Padding="0" />
            </Grid>
            <Grid Grid.Column="1" Grid.Row="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Button x:Name="btnLastYear" Grid.Column="0" HorizontalContentAlignment="Center"
                        VerticalContentAlignment="Center" Click="btnLastYear_Click">
                    <TextBlock Margin="-2">전년</TextBlock>
                </Button>
                <Button x:Name="btnThisYear" Grid.Column="1" HorizontalContentAlignment="Center"
                        VerticalContentAlignment="Center" Click="btnThisYear_Click">
                    <TextBlock Margin="-2">금년</TextBlock>
                </Button>
                <Button x:Name="btnLastSixMonth" Grid.Column="2" HorizontalContentAlignment="Center"
                        VerticalContentAlignment="Center" Click="btnLastSixMonth_Click">
                    <TextBlock Margin="-3">최근6개월</TextBlock>
                </Button>
                <Button x:Name="btnThisMonth" Grid.Column="3" HorizontalContentAlignment="Center"
                        VerticalContentAlignment="Center" Click="btnThisMonth_Click">
                    <TextBlock Margin="-2">금월</TextBlock>
                </Button>
            </Grid>

            <Grid Grid.Column="2" Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="75*" />
                    <ColumnDefinition Width="90*" />
                    <ColumnDefinition Width="15*" />
                </Grid.ColumnDefinitions>
                <Label x:Name="lblDefectOccurStep" Grid.Column="0" Style ="{DynamicResource LabelSearch_Q}" >
                    <StackPanel Style ="{DynamicResource StackPanelInLabel_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <CheckBox x:Name="chkDefectOccurStep" IsChecked="True" IsEnabled="False"
                                      Checked="chkDefectOccurStep_Checked" Unchecked="chkDefectOccurStep_Unchecked"/>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock Text=" 불량발생단계" Style="{DynamicResource TextBlockInViewBoxSearch_Q}"/>
                        </Viewbox>
                    </StackPanel>
                </Label>
                <ComboBox x:Name="cboOccurStepSrh" Grid.Column="1" Background="White" IsEnabled="True"/>
            </Grid>
            <Grid Grid.Column="2" Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="75*" />
                    <ColumnDefinition Width="90*" />
                    <ColumnDefinition Width="15*" />
                </Grid.ColumnDefinitions>
                <Label x:Name="lblCustom" Grid.Column="0" MouseLeftButtonUp="lblCustom_MouseLeftButtonUp" Style="{DynamicResource LabelSearch_Q}">
                    <StackPanel Style="{DynamicResource StackPanelInLabel_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <CheckBox x:Name="chkCustom"
                                      Checked="chkCustom_Checked" Unchecked="chkCustom_Unchecked"/>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock Text=" 고 객 사" Style="{DynamicResource TextBlockInViewBoxSearch_Q}"/>
                        </Viewbox>
                    </StackPanel>
                </Label>
                <TextBox x:Name="txtCustom" Grid.Column="1" Background="White"
                         Keyboard.KeyDown="txtCustom_KeyDown" IsEnabled="False"/>
                <Button x:Name="btnPfCustom" Grid.Column="2" 
                        Click="btnPfCustom_Click" IsEnabled="False">
                    <Image Source="{DynamicResource btiPlusFind}" />
                </Button>
            </Grid>
            <Grid Grid.Column="2" Grid.Row="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="75*" />
                    <ColumnDefinition Width="90*" />
                    <ColumnDefinition Width="15*" />
                </Grid.ColumnDefinitions>
                <Label x:Name="lblArticle" Grid.Column="0" 
                       MouseLeftButtonUp="lblArticle_MouseLeftButtonUp" Style="{DynamicResource LabelSearch_Q}">
                    <StackPanel Style="{DynamicResource StackPanelInLabel_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <CheckBox x:Name="chkArticle"
                                      Checked="chkArticle_Checked" Unchecked="chkArticle_Unchecked"/>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock Text=" 품     번" Style="{DynamicResource TextBlockInViewBoxSearch_Q}"/>
                        </Viewbox>
                    </StackPanel>
                </Label>
                <TextBox x:Name="txtArticle" Grid.Column="1" Background="White"
                         Keyboard.KeyDown="txtArticle_KeyDown" IsEnabled="False"/>
                <Button x:Name="btnPfArticle" Grid.Column="2" 
                        Click="btnPfArticle_Click" IsEnabled="False">
                    <Image Source="{DynamicResource btiPlusFind}" />
                </Button>
            </Grid>

            <Grid Grid.Column="4" Grid.RowSpan="4">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Button x:Name="btnSearch" Grid.Column="2"  Click="btnSearch_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiSearch}" Style="{DynamicResource imgSize}" VerticalAlignment="Center"/>
                            <TextBlock Text="{Binding Source={StaticResource tblSearch},Mode=OneWay,Path=Text}" VerticalAlignment="Center"/>
                        </StackPanel>
                    </Button>
                    <Button x:Name="btnClose" Grid.Column="3"  Click="btnClose_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiClose}" Style="{DynamicResource imgSize}" VerticalAlignment="Center"/>
                            <TextBlock Text="{Binding Source={StaticResource tblClose},Mode=OneWay,Path=Text}" VerticalAlignment="Center"/>
                        </StackPanel>
                    </Button>
                </Grid>
                <Grid Grid.Row="1" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Button x:Name="btnPrint" Grid.Column="0" Margin="1" Click="btnPrint_Click"
                            Visibility="Collapsed">
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiPrint}" Style="{DynamicResource imgSize}" VerticalAlignment="Center"/>
                            <TextBlock Text="{Binding Source={StaticResource tblPrint},Mode=OneWay,Path=Text}" VerticalAlignment="Center"/>
                        </StackPanel>
                    </Button>

                    <Button x:Name="btnExcel" Grid.Column="4"  Click="btnExcel_Click" >
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiExcel}" Style="{DynamicResource imgSize}" VerticalAlignment="Center"/>
                            <TextBlock Text="{Binding Source={StaticResource tblExcel},Mode=OneWay,Path=Text}" VerticalAlignment="Center"/>
                        </StackPanel>
                    </Button>
                </Grid>
            </Grid>
        </Grid>
        <Grid Grid.Row="1" Grid.RowSpan="2">
            <UniformGrid Columns="3" Margin="5,0">
                <FrameworkElement x:Name="c1"/>
            </UniformGrid>
            <TabControl x:Name="tabconGrid" Grid.ColumnSpan="3">
                <TabItem Header="전체" Width="{Binding Path=ActualWidth, ElementName=c1}" Height="25" MouseLeftButtonUp="All_MouseLeftButtonUp">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="9*"/>
                            <RowDefinition Height="218*"/>
                            <RowDefinition Height="50"/>
                        </Grid.RowDefinitions>
                        <Grid Grid.Column="0" Grid.RowSpan="2">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0.12*"/>
                                <RowDefinition Height="2*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="0.8*" />
                                <ColumnDefinition Width="1.2*" />
                            </Grid.ColumnDefinitions>
                            <Label Grid.Row="0" Grid.Column="0" Margin="1"
                                   HorizontalContentAlignment="Center" VerticalContentAlignment="Center">
                                <TextBlock Text="불량유형별" />
                            </Label>
                            <Button x:Name="btnSymptomGraph" Grid.Row="0" Grid.Column="1" Margin="1"
                                    Click="btnSymptomGraph_Click">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="※※※ " Foreground="BlueViolet"/>
                                    <TextBlock Text="유형별 불량 도표보기" />
                                </StackPanel>
                            </Button>
                            <DataGrid x:Name="dgdAllDefectSymptom" Grid.Row="1" Grid.Column="0" 
                                      Grid.ColumnSpan="2" Style="{DynamicResource DataGridSet1}" >
                                <DataGrid.RowStyle>
                                    <Style TargetType="DataGridRow">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Path=ColorBlue}" Value="true">
                                                <Setter Property="Background" Value="#365fb1" />
                                                <Setter Property="Foreground" Value="White" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </DataGrid.RowStyle>
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="" Binding="{Binding Num}"/>
                                    <DataGridTextColumn Header="불량유형" Binding="{Binding GroupingName}"/>
                                    <DataGridTextColumn Header="수량" Binding="{Binding DefectQty}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="시정수" Binding="{Binding RepairQty}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="시정률(%)" Binding="{Binding RepairRate}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="점유율(%)" Binding="{Binding RepairRate1}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                </DataGrid.Columns>
                            </DataGrid>

                        </Grid>

                        <Grid Grid.Column="1" Grid.RowSpan="2">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0.12*"/>
                                <RowDefinition Height="2*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="0.8*" />
                                <ColumnDefinition Width="1.2*" />
                            </Grid.ColumnDefinitions>
                            <Label Grid.Row="0" Grid.Column="0" Margin="1"
                                   HorizontalContentAlignment="Center" VerticalContentAlignment="Center">
                                <TextBlock Text="불량원인별" />
                            </Label>
                            <Button x:Name="btnReasonGraph" Grid.Row="0" Grid.Column="1" Margin="1"
                                    Click="btnReasonGraph_Click">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="※※※ " Foreground="BlueViolet"/>
                                    <TextBlock Text="원인별 불량 도표보기" />
                                </StackPanel>
                            </Button>
                            <DataGrid x:Name="dgdAllDefectReason" Grid.Row="1" Grid.Column="0" 
                                      Grid.ColumnSpan="2"  Style="{DynamicResource DataGridSet1}">
                                <DataGrid.RowStyle>
                                    <Style TargetType="DataGridRow">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Path=ColorBlue}" Value="true">
                                                <Setter Property="Background" Value="#365fb1" />
                                                <Setter Property="Foreground" Value="White" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </DataGrid.RowStyle>
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="" Binding="{Binding Num}"/>
                                    <DataGridTextColumn Header="불량원인" Binding="{Binding GroupingName}"/>
                                    <DataGridTextColumn Header="수량" Binding="{Binding DefectQty}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="시정수" Binding="{Binding RepairQty}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="시정률(%)" Binding="{Binding RepairRate}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="점유율(%)" Binding="{Binding RepairRate1}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                </DataGrid.Columns>
                            </DataGrid>
                        </Grid>
                        <Grid Grid.Column="2" Grid.RowSpan="2">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0.12*"/>
                                <RowDefinition Height="2*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="0.8*" />
                                <ColumnDefinition Width="1.2*" />
                            </Grid.ColumnDefinitions>
                            <Label Grid.Row="0" Grid.Column="0" Margin="1"
                                   HorizontalContentAlignment="Center" VerticalContentAlignment="Center">
                                <TextBlock Text="업체별" />
                            </Label>
                            <Button x:Name="btnCustomGraph" Grid.Row="0" Grid.Column="1" Margin="1"
                                    Click="btnCustomGraph_Click">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="※※※ " Foreground="BlueViolet"/>
                                    <TextBlock Text="업체별 불량 도표보기" />
                                </StackPanel>
                            </Button>
                            <DataGrid x:Name="dgdAllDefectCustom" Grid.Row="1" Grid.Column="0" 
                                      Grid.ColumnSpan="2" Style="{DynamicResource DataGridSet1}">
                                <DataGrid.RowStyle>
                                    <Style TargetType="DataGridRow">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Path=ColorBlue}" Value="true">
                                                <Setter Property="Background" Value="#365fb1" />
                                                <Setter Property="Foreground" Value="White" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </DataGrid.RowStyle>
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="" Binding="{Binding Num}"/>
                                    <DataGridTextColumn Header="업체명" Binding="{Binding GroupingName}"/>
                                    <DataGridTextColumn Header="수량" Binding="{Binding DefectQty}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="시정수" Binding="{Binding RepairQty}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="시정률(%)" Binding="{Binding RepairRate}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                    <DataGridTextColumn Header="점유율(%)" Binding="{Binding RepairRate1}" CellStyle="{StaticResource NumericDataGridCell}"/>
                                </DataGrid.Columns>
                            </DataGrid>
                        </Grid>
                        
                        <DataGrid x:Name="dgdTotal" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3" Style="{DynamicResource DataGridSet1}" >
                            <DataGrid.Columns>
                                <DataGridTextColumn Header="" Binding="{Binding GroupingName}" Width="50" CellStyle="{StaticResource DataGridCellAndCentering}" />
                                <DataGridTextColumn Header="수량" Binding="{Binding DefectQty}" CellStyle="{StaticResource NumericDataGridCell}" Width="*"/>
                                <DataGridTextColumn Header="시정수" Binding="{Binding RepairQty}" CellStyle="{StaticResource NumericDataGridCell}" Width="*"/>
                                <DataGridTextColumn Header="시정률(%)" Binding="{Binding RepairRate}" CellStyle="{StaticResource NumericDataGridCell}" Width="*"/>
                            </DataGrid.Columns>
                        </DataGrid>

                    </Grid>

                </TabItem>
                <TabItem Header="일별" Width="{Binding Path=ActualWidth, ElementName=c1}" Height="25" MouseLeftButtonUp="TabItem_MouseLeftButtonUp">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1.5*"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1.3*" />
                                <RowDefinition Height="1.2*" />
                            </Grid.RowDefinitions>
                            <lvc:CartesianChart x:Name="lvcDayChart" Grid.Row="0" Margin="1" Series="{Binding SeriesCollection}">
                            </lvc:CartesianChart>

                            <Border Grid.Row="1" Margin="1">
                                <DataGrid x:Name="dgdDefectRepairDaily" Style="{DynamicResource DataGridSet1}" >
                                    <DataGrid.Columns>
                                        <DataGridTemplateColumn Header="순" >
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                        </Grid.RowDefinitions>
                                                        <TextBlock Grid.Row="0" Text="{Binding Num1}" Margin="3"
                                                               HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                        <Line Grid.Row="1" Stroke="Black" StrokeThickness="2" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="2" Text="{Binding Num2}" Margin="3"
                                                               HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                        <Line Grid.Row="3" Stroke="Black" StrokeThickness="1" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="4" Text="{Binding Num3}" Margin="3"
                                                               HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                        <Line Grid.Row="5" Stroke="Black" StrokeThickness="1" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="6" Text="{Binding Num4}" Margin="3"
                                                               HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                    </Grid>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn Header="구분" >
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                        </Grid.RowDefinitions>
                                                        <TextBlock Grid.Row="0" Text="{Binding strGubun1}" Margin="3"
                                                               HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                        <Line Grid.Row="1" Stroke="Black" StrokeThickness="2" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="2" Text="{Binding strGubun2}" Margin="3"
                                                               HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                        <Line Grid.Row="3" Stroke="Black" StrokeThickness="1" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="4" Text="{Binding strGubun3}" Margin="3"
                                                               HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                        <Line Grid.Row="5" Stroke="Black" StrokeThickness="1" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="6" Text="{Binding strGubun4}" Margin="3"
                                                               HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                    </Grid>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn x:Name="dgdtpeDate01" Header="1일" >
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                        </Grid.RowDefinitions>
                                                        <TextBlock Grid.Row="0" Text="{Binding ProdQty1}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center" />
                                                        <Line Grid.Row="1" Stroke="Black" StrokeThickness="2" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="2" Text="{Binding DefectQty1}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                        <Line Grid.Row="3" Stroke="Black" StrokeThickness="1" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="4" Text="{Binding RepairQty1}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                        <Line Grid.Row="5" Stroke="Black" StrokeThickness="1" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="6" Text="{Binding RepairRate1}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                    </Grid>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn x:Name="dgdtpeDate02" Header="2일" >
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                        </Grid.RowDefinitions>
                                                        <TextBlock Grid.Row="0" Text="{Binding ProdQty2}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                        <Line Grid.Row="1" Stroke="Black" StrokeThickness="2" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="2" Text="{Binding DefectQty2}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                        <Line Grid.Row="3" Stroke="Black" StrokeThickness="1" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="4" Text="{Binding RepairQty2}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                        <Line Grid.Row="5" Stroke="Black" StrokeThickness="1" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="6" Text="{Binding RepairRate2}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                    </Grid>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn x:Name="dgdtpeDate03" Header="3일" >
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                            <RowDefinition Height="auto"/>
                                                        </Grid.RowDefinitions>
                                                        <TextBlock Grid.Row="0" Text="{Binding ProdQty3}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                        <Line Grid.Row="1" Stroke="Black" StrokeThickness="2" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="2" Text="{Binding DefectQty3}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                        <Line Grid.Row="3" Stroke="Black" StrokeThickness="1" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="4" Text="{Binding RepairQty3}" Margin="3"
                                                               HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                        <Line Grid.Row="5" Stroke="Black" StrokeThickness="1" 
                                                          Stretch="Fill" VerticalAlignment="Center" X2="1"/>
                                                        <TextBlock Grid.Row="6" Text="{Binding RepairRate3}" Margin="3"
