        <Grid Grid.Row="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <!--<Button x:Name="btnCapture" Grid.Column="0" Click="BtnCapture_Click">
                        <StackPanel Orientation="Horizontal" Margin="-2">
                            <Image Source="{DynamicResource btiPrint}" Style="{DynamicResource imgSize}" VerticalAlignment="Center"/>
                            <TextBlock Text="Capture" VerticalAlignment="Center"/>
                        </StackPanel>
                    </Button>
                    <Image x:Name="ImgImage" Width="820" Height="620" Visibility="Hidden"/>-->
        </Grid>
        <Grid Grid.Row="1">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="250*"/>
                    <RowDefinition Height="282*"/>
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="420*" />
                        <ColumnDefinition Width="400*" />
                    </Grid.ColumnDefinitions>

                    <Grid Grid.Column="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="25" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <Label x:Name="lblGreenPoint" Grid.Row="0" Content="1. 월별 불량지수"  Background="LimeGreen" Style="{DynamicResource LabelInputArea_Q}" ></Label>
                        <lvc:CartesianChart x:Name="lvcDayChart" Grid.Row="1" Margin="1" Series="{Binding SeriesCollection}" >
                            <lvc:CartesianChart.AxisX>
                                <lvc:Axis Title="Month" Labels="{Binding X_Linelbl}"></lvc:Axis>
                            </lvc:CartesianChart.AxisX>
                        </lvc:CartesianChart>
                    </Grid>
                    <Grid Grid.Column="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="25"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>

                        <Label Grid.Row="0" x:Name="lblchartMonth" Background="LimeGreen" Style="{DynamicResource LabelInputArea_Q}" ></Label>
                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="40" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>

                            <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1" Margin="1">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="30" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <Label Grid.Row="0" FontSize="7"  Style="{DynamicResource LabelInputArea_Q}">
                                        <Viewbox  Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                                            <TextBlock Text="제품별 점유율(%)" Style="{DynamicResource TextBlockInViewBoxInput_Title_Q}"/>
                                        </Viewbox>
                                    </Label>
                                    <lvc:PieChart x:Name="lvcProductPieChart" Grid.Row="1" Margin="1" >
                                    </lvc:PieChart>
                                </Grid>
                            </Border>

                            <Grid Grid.Column="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>

                                <Image Grid.Row="1" Source="{DynamicResource btiAddRight}"></Image>
                            </Grid>

                            <Border Grid.Column="2" BorderBrush="Black" BorderThickness="1" Margin="1">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="30" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <Label Grid.Row="0" FontSize="7" Style="{DynamicResource LabelInputArea_Q}" >
                                        <Viewbox  Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                                            <TextBlock Text="유형별 점유율(%)" Style="{DynamicResource TextBlockInViewBoxInput_Title_Q}"/>
                                        </Viewbox>
                                    </Label>
                                    <lvc:PieChart x:Name="lvcTypePieChart" Grid.Row="1" Margin="1">

                                    </lvc:PieChart>
                                </Grid>
                            </Border>
                        </Grid>
                    </Grid>
                </Grid>


                <Grid Grid.Row="1" Margin="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="4*"/>
                        <ColumnDefinition Width="51*"/>
                        <ColumnDefinition Width="55*"/>
                        <ColumnDefinition Width="55*"/>
                        <ColumnDefinition Width="55*"/>
                        <ColumnDefinition Width="55*"/>
                        <ColumnDefinition Width="55*"/>
                        <ColumnDefinition Width="55*"/>
                        <ColumnDefinition Width="55*"/>
                        <ColumnDefinition Width="54*"/>
                        <ColumnDefinition Width="54*"/>
                        <ColumnDefinition Width="54*"/>
                        <ColumnDefinition Width="54*"/>
                        <ColumnDefinition Width="54*"/>
                        <ColumnDefinition Width="54*"/>
                        <ColumnDefinition Width="54*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="15*"/>
                        <RowDefinition Height="90*"/>
                        <RowDefinition Height="20*"/>
                        <RowDefinition Height="15*"/>
                        <RowDefinition Height="45*"/>
                        <RowDefinition Height="45*"/>
                        <RowDefinition Height="45*"/>
                    </Grid.RowDefinitions>

                    <Label x:Name="lblGubun" Grid.Column="0" Grid.ColumnSpan="3" Grid.Row="0"
                           Style="{DynamicResource LabelInputArea_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock Text="구  분" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_TwoBlock_Q}"/>
                        </Viewbox>
                    </Label>
                    <Label  Grid.Column="3" Grid.Row="0" Style="{DynamicResource LabelInputArea_Q}" >
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock x:Name="lbl_1Month" Text="1월" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_OneBlock_Q}"/>
                        </Viewbox>
                    </Label>
                    <Label Grid.Column="4" Grid.Row="0" Style="{DynamicResource LabelInputArea_Q}" >
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock x:Name="lbl_2Month" Text="2월" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_OneBlock_Q}"/>
                        </Viewbox>
                    </Label>
                    <Label Grid.Column="5" Grid.Row="0" Style="{DynamicResource LabelInputArea_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock x:Name="lbl_3Month" Text="3월" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_OneBlock_Q}"/>
                        </Viewbox>
                    </Label>
                    <Label Grid.Column="6" Grid.Row="0" Style="{DynamicResource LabelInputArea_Q}" >
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock x:Name="lbl_4Month" Text="4월" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_OneBlock_Q}"/>
                        </Viewbox>
                    </Label>
                    <Label Grid.Column="7" Grid.Row="0" Style="{DynamicResource LabelInputArea_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock x:Name="lbl_5Month" Text="5월" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_OneBlock_Q}"/>
                        </Viewbox>
                    </Label>
                    <Label Grid.Column="8" Grid.Row="0" Style="{DynamicResource LabelInputArea_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock x:Name="lbl_6Month" Text="6월" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_OneBlock_Q}"/>
                        </Viewbox>
                    </Label>
                    <Label Grid.Column="9" Grid.Row="0" Style="{DynamicResource LabelInputArea_Q}" >
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock x:Name="lbl_7Month" Text="7월" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_OneBlock_Q}"/>
                        </Viewbox>
                    </Label>
                    <Label Grid.Column="10" Grid.Row="0" Style="{DynamicResource LabelInputArea_Q}" >
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock x:Name="lbl_8Month" Text="8월" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_OneBlock_Q}"/>
                        </Viewbox>
                    </Label>
                    <Label Grid.Column="11" Grid.Row="0" Style="{DynamicResource LabelInputArea_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock x:Name="lbl_9Month" Text="9월" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_OneBlock_Q}"/>
                        </Viewbox>
                    </Label>
                    <Label Grid.Column="12" Grid.Row="0" Style="{DynamicResource LabelInputArea_Q}" >
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock x:Name="lbl_10Month" Text="10월" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_OneBlock_Q}"/>
                        </Viewbox>
                    </Label>
                    <Label Grid.Column="13" Grid.Row="0" Style="{DynamicResource LabelInputArea_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock x:Name="lbl_11Month" Text="11월" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_OneBlock_Q}"/>
                        </Viewbox>
                    </Label>
                    <Label Grid.Column="14" Grid.Row="0" Style="{DynamicResource LabelInputArea_Q}" >
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock x:Name="lbl_12Month" Text="12월" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_OneBlock_Q}"/>
                        </Viewbox>
                    </Label>
                    <Label Grid.Column="15" Grid.Row="0" Style="{DynamicResource LabelInputArea_Q}"
                           HorizontalContentAlignment="Center">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock Text="합  계" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_OneBlock_Q}"/>
                        </Viewbox>
                    </Label>
                    <Label x:Name="lblDefectCount" Grid.Row="1" Style="{DynamicResource LabelInputArea_Q}" Grid.ColumnSpan="2" >
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock Text="불량지수&#x0a;  (건수)" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_OneBlock_Long_Q}"/>
                        </Viewbox>
                    </Label>

                    <Viewbox Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="10" HorizontalAlignment="Left" Style="{DynamicResource ViewBoxSetMaxMinHeight}" >
                        <TextBlock Text=" ※ 구분별로 최대 상위12개의 불량정보가 아래에 표기됩니다" FontWeight="Bold" Foreground="Blue" Margin="0,5,0,1"/>
                    </Viewbox>

                    <Label Grid.Column="0" Grid.ColumnSpan="3" Grid.Row="3"
                           Style="{DynamicResource LabelInputArea_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock Text="구  분" Style="{DynamicResource TextBlockInViewBoxInput_AllSector_TwoBlock_Q}"/>
                        </Viewbox>
