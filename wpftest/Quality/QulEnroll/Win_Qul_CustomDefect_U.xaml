<UserControl x:Class="WizMes_ParkPro.Win_Qul_CustomDefect_U"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WizMes_ParkPro"
             mc:Ignorable="d" 
             d:DesignHeight="620" d:DesignWidth="820" Loaded="Win_Qul_CustomDefect_U_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="88*"/>
            <RowDefinition Height="532*"/>
        </Grid.RowDefinitions>

        <Grid Grid.Column="0" Grid.Row="0" Background="{DynamicResource WizMes_ParkPro}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="100*" />
                <ColumnDefinition Width="160*" />
                <ColumnDefinition Width="180*" />
                <ColumnDefinition Width="180*" />
                <ColumnDefinition Width="200*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
            </Grid.RowDefinitions>

            <Grid Grid.Column="1" Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="65*"/>
                    <ColumnDefinition Width="85*"/>
                </Grid.ColumnDefinitions>

                <Label  Grid.Column="0" Grid.Row="0" MouseLeftButtonUp="chkSearchDay_Click"  Style="{DynamicResource LabelSearch_Q}">
                    <StackPanel Style="{DynamicResource StackPanelInLabel_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <CheckBox x:Name="chkSearchDay" Click="chkSearchDay_Click" IsChecked="True"></CheckBox>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock Text="일   자" Style="{DynamicResource TextBlockInViewBoxSearch_Q}"/>
                        </Viewbox>
                    </StackPanel>
                </Label>
                <DatePicker x:Name="dtpFromDate" Grid.Column="1"  Style="{DynamicResource DatePickerSearch_Q}" IsEnabled="True" />
            </Grid>
            <Grid Grid.Column="1" Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="65*"/>
                    <ColumnDefinition Width="85*"/>
                </Grid.ColumnDefinitions>

                <DatePicker x:Name="dtpToDate" Grid.Column="1" Padding="0"  Style="{DynamicResource DatePickerSearch_Q}" IsEnabled="True" />
            </Grid>
            <Grid Grid.Column="1" Grid.Row="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Button x:Name="btnLastMonth" Grid.Column="0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="전월" Margin="1" Click="btnLastMonth_Click"/>
                <Button x:Name="btnThisMonth" Grid.Column="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="금월" Margin="1" Click="btnThisMonth_Click" />
                <Button x:Name="btnYesterDay" Grid.Column="2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="전일" Margin="1" Click="btnYesterDay_Click"/>
                <Button x:Name="btnToday" Grid.Column="3" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="금일" Margin="1" Click="btnToday_Click"/>
            </Grid>
            <Border Grid.Column="1" Grid.Row="3"  HorizontalAlignment="Stretch" Background="#ededed" BorderBrush="Gray" BorderThickness="1">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}" Grid.Column="0" >
                        <RadioButton x:Name="rbnOccurDay" HorizontalAlignment="Left" VerticalAlignment="Center" Content="발생일" Margin="1" Click="rbnOccurDay_Click"/>
                    </Viewbox>
                    <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}" Grid.Column="1" >
                        <RadioButton x:Name="rbnReceiptDay"  HorizontalAlignment="Left" VerticalAlignment="Center" Content="접수일" Margin="1" Click="rbnReceiptDay_Click"/>
                    </Viewbox>
                    <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}" Grid.Column="2" >
                        <RadioButton x:Name="rbnActionDay"  HorizontalAlignment="Left" VerticalAlignment="Center" Content="조치일" Margin="1" Click="rbnActionDay_Click"/>
                    </Viewbox>
                </Grid>
            </Border>
            <Grid Grid.Column="2" Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="75*" />
                    <ColumnDefinition Width="90*" />
                    <ColumnDefinition Width="15*" />
                </Grid.ColumnDefinitions>

                <Label  Grid.Column="0" MouseLeftButtonUp="chkCustomer_Click" Style="{DynamicResource LabelSearch_Q}">
                    <StackPanel  Style="{DynamicResource StackPanelInLabel_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <CheckBox x:Name="chkCustomer" Click="chkCustomer_Click"></CheckBox>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock Text="거 래 처"  Style="{DynamicResource TextBlockInViewBoxSearch_Q}"/>
                        </Viewbox>
                    </StackPanel>
                </Label>
                <TextBox x:Name="txtCustomer" Grid.Column="1" Style="{DynamicResource TextBoxSearch_Q}" KeyDown="txtCustomer_KeyDown"/>
                <Button x:Name="btnCustomer"   Grid.Column="2" Click="btnCustomer_Click"  >
                    <Image Source="{DynamicResource btiPlusFind}" Style="{DynamicResource imgPlusFind}"/>
                </Button>
            </Grid>
            <Grid Grid.Column="2" Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="75*" />
                        <ColumnDefinition Width="90*" />
                        <ColumnDefinition Width="15*" />
                    </Grid.ColumnDefinitions>
                    <Label  Grid.Column="0" MouseLeftButtonUp="chkArticle_Click" Style="{DynamicResource LabelSearch_Q}">
                        <StackPanel Orientation="Horizontal" Style="{DynamicResource StackPanelInLabel_Q}">
                            <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                                <CheckBox x:Name="chkArticle" Click="chkArticle_Click" ></CheckBox>
                            </Viewbox>
                            <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                                <TextBlock Text="품     명" Style="{DynamicResource TextBlockInViewBoxSearch_Q}"/>
                            </Viewbox>
                        </StackPanel>
                    </Label>
                    <TextBox x:Name="txtArticle" Grid.Column="1" KeyDown="TxtArticle_KeyDown" Style="{DynamicResource TextBoxSearch_Q}"/>
                    <Button x:Name="btnArticle"  Grid.Column="2" Click="btnArticle_Click"  >
                        <Image Source="{DynamicResource btiPlusFind}" Style="{DynamicResource imgPlusFind}"/>
                    </Button>
                
            </Grid>

            <Grid Grid.Column="3" Grid.Row="1" Visibility="Hidden">
                <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="75*" />
                        <ColumnDefinition Width="90*" />
                        <ColumnDefinition Width="15*" />
                    </Grid.ColumnDefinitions>
                    <Label  Grid.Column="0" MouseLeftButtonUp="chkArticleNo_Click" Style="{DynamicResource LabelSearch_Q}">
                        <StackPanel Orientation="Horizontal" Style="{DynamicResource StackPanelInLabel_Q}">
                            <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                                <CheckBox x:Name="chkArticleNo" Click="chkArticleNo_Click" ></CheckBox>
                            </Viewbox>
                            <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                                <TextBlock Text="품     번" Style="{DynamicResource TextBlockInViewBoxSearch_Q}"/>
                            </Viewbox>
                        </StackPanel>
                    </Label>
                    <TextBox x:Name="txtArticleNo" Grid.Column="1" KeyDown="TxtArticleNo_KeyDown" Style="{DynamicResource TextBoxSearch_Q}"/>
                    <Button x:Name="btnArticleNo"  Grid.Column="2" Click="btnArticleNo_Click"  >
                        <Image Source="{DynamicResource btiPlusFind}" Style="{DynamicResource imgPlusFind}"/>
                    </Button>
            </Grid>
            
            <Grid Grid.Column="2" Grid.Row="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="75*" />
                    <ColumnDefinition Width="90*" />
                    <ColumnDefinition Width="15*" />
                </Grid.ColumnDefinitions>

                <Label  Grid.Column="0" MouseLeftButtonUp="chkDefectContent_Click" Style="{DynamicResource LabelSearch_Q}">
                    <StackPanel Style="{DynamicResource StackPanelInLabel_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <CheckBox x:Name="chkDefectContent" Click="chkDefectContent_Click"></CheckBox>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock Text="발생내역(%)" Style="{DynamicResource TextBlockInViewBoxSearch_Q}"/>
                        </Viewbox>
                    </StackPanel>
                </Label>
                <TextBox x:Name="txtDefectContent" Grid.Column="1" Style="{DynamicResource TextBoxSearch_Q}"/>
            </Grid>
            <Grid Grid.Column="2" Grid.Row="3">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="75*" />
                    <ColumnDefinition Width="90*" />
                    <ColumnDefinition Width="15*" />
                </Grid.ColumnDefinitions>

                <Label  Grid.Column="0" MouseLeftButtonUp="chkDefectNO_Click" Style="{DynamicResource LabelSearch_Q}">
                    <StackPanel Style="{DynamicResource StackPanelInLabel_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <CheckBox x:Name="chkDefectNO" Click="chkDefectNO_Click" ></CheckBox>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock Text="불량번호"  Style="{DynamicResource TextBlockInViewBoxSearch_Q}"/>
                        </Viewbox>
                    </StackPanel>
                </Label>
                <TextBox x:Name="txtDefectNO" Grid.Column="1" Style="{DynamicResource TextBoxSearch_Q}"/>
                
            </Grid>
            <Grid Grid.Column="3" Grid.Row="0" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="20*" />
                </Grid.ColumnDefinitions>
                <Label Style="{DynamicResource LabelSearch_Q}">
                    <StackPanel Style="{DynamicResource StackPanelInLabel_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}"  >
                            <RadioButton x:Name="rbnChoice2"  Content="양   산" Click="rbnChoice2_Click" Margin="0 0 20 0"/>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}"  >
                            <RadioButton x:Name="rbnChoice1"  Content="개   발" Click="rbnChoice1_Click" Margin="0 0 20 0" />
                        </Viewbox>
                    </StackPanel>
                </Label>
                    
            </Grid>
            
            <Grid Grid.Column="3" Grid.Row="3">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="75*" />
                    <ColumnDefinition Width="90*" />
                    <ColumnDefinition Width="15*" />
                </Grid.ColumnDefinitions>
                <Label  Grid.Column="0" MouseLeftButtonUp="chkCloseYN_Click" Style="{DynamicResource LabelSearch_Q}">
                    <StackPanel Style="{DynamicResource StackPanelInLabel_Q}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <CheckBox x:Name="chkCloseYN" Click="chkCloseYN_Click" ></CheckBox>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                            <TextBlock Text="종결여부" Style="{DynamicResource TextBlockInViewBoxSearch_Q}"/>
                        </Viewbox>
                    </StackPanel>
                </Label>
                <ComboBox x:Name="cboCloseYN" Grid.Column="1"  Style="{DynamicResource ComboBoxSearchString_Q}"/>
            </Grid>
          


            <Grid Grid.Column="5" Grid.ColumnSpan="4" Grid.RowSpan="4">
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>

                <!-- 검색버튼 -->
                <Grid Grid.Column="0" Grid.Row="1" >
                    <Button x:Name="btnSearch" Click="btnSearch_Click"  Margin="1" >
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiSearch}" Style="{DynamicResource imgSize}" VerticalAlignment="Center" />
                            <TextBlock Text="{Binding Source={StaticResource tblSearch},Mode=OneWay,Path=Text}" VerticalAlignment="Center" />
                        </StackPanel>
                    </Button>
                </Grid>

                <!-- 추가버튼 -->
                <Grid Grid.Column="0">
                    <Button x:Name="btnAdd"  Tag="1" Click="btnAdd_Click" Margin="1" >
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiInsert}" Style="{DynamicResource imgSize}" VerticalAlignment="Center"/>
                            <TextBlock Text="{Binding Source={StaticResource tblInsert},Mode=OneWay,Path=Text}" VerticalAlignment="Center" />
                        </StackPanel>
                    </Button>
                </Grid>

                <!-- 수정버튼 -->
                <Grid Grid.Column="1" >
                    <Button x:Name="btnEdit" Tag="2" Click="btnEdit_Click" Margin="1" >
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiUpdate}" Style="{DynamicResource imgSize}" VerticalAlignment="Center"/>
                            <TextBlock Text="{Binding Source={StaticResource tblUpdate},Mode=OneWay,Path=Text}" VerticalAlignment="Center" />
                        </StackPanel>
                    </Button>
                </Grid>

                <!-- 삭제버튼 -->
                <Grid Grid.Column="2" >
                    <Button x:Name="btnDelete"  Click="btnDelete_Click" Margin="1" >
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiDelete}" Style="{DynamicResource imgSize}" VerticalAlignment="Center"/>
                            <TextBlock Text="{Binding Source={StaticResource tblDelete},Mode=OneWay,Path=Text}" VerticalAlignment="Center" />
                        </StackPanel>
                    </Button>
                </Grid>

                <!-- 닫기버튼 -->
                <Grid Grid.Column="3" >
                    <Button x:Name="btnClose"  Click="btnClose_Click" Margin="1" >
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiClose}" Style="{DynamicResource imgSize}" VerticalAlignment="Center" />
                            <TextBlock Text="{Binding Source={StaticResource tblClose},Mode=OneWay,Path=Text}" VerticalAlignment="Center" />
                        </StackPanel>
                    </Button>
                </Grid>

                <!-- 저장버튼 -->
                <Grid Grid.Column="1" Grid.Row="1" >
                    <Button x:Name="btnSave"  Click="btnSave_Click" Margin="1" >
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiSave}" Style="{DynamicResource imgSize}" VerticalAlignment="Center" />
                            <TextBlock Text="{Binding Source={StaticResource tblSave},Mode=OneWay,Path=Text}" VerticalAlignment="Center" />
                        </StackPanel>
                    </Button>
                </Grid>

                <!-- 취소 버튼 -->
                <Grid Grid.Column="2" Grid.Row="1">
                    <Button x:Name="btnCancel"  Click="btnCancel_Click" Margin="1" >
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiCancle}" Style="{DynamicResource imgSize}" VerticalAlignment="Center" />
                            <TextBlock Text="{Binding Source={StaticResource tblCancle},Mode=OneWay,Path=Text}" VerticalAlignment="Center" />
                        </StackPanel>
                    </Button>
                </Grid>

                <!-- 엑셀버튼 -->
                <Grid Grid.Column="3" Grid.Row="1" >
                    <Button x:Name="btnExcel"  Click="btnExcel_Click"  Margin="1">
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiExcel}" Style="{DynamicResource imgSize}" VerticalAlignment="Center"/>
                            <TextBlock Text="{Binding Source={StaticResource tblExcel},Mode=OneWay,Path=Text}" VerticalAlignment="Center" />
                        </StackPanel>
                    </Button>
                </Grid>

               

            </Grid>

        </Grid>



        <Grid Grid.Column="0" Grid.Row="1" Margin="2" >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="200*" />
                <ColumnDefinition Width="auto" />
                <ColumnDefinition Width="180*"/>
                <ColumnDefinition Width="auto" />
                <ColumnDefinition Width="180*" />
            </Grid.ColumnDefinitions>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="25*"/>
                    <RowDefinition Height="1*"/>
                </Grid.RowDefinitions>
                <DataGrid x:Name="dgdCustomDefect" Grid.Column="0" Grid.Row="0" SelectionChanged="dgdCustomDefect_SelectionChanged" Style="{DynamicResource DataGridSet1}" >
                <DataGrid.Columns>
                    <DataGridTextColumn Header="불량ID" Width="auto" MinWidth="60" Binding="{Binding Path=DefectID}"
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="발생일" Width="auto" MinWidth="60" Binding="{Binding Path=OccurDate}"
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="불량번호" Width="auto" MinWidth="60" Binding="{Binding Path=DefectNo}" />
                    <DataGridTextColumn Header="고객사" Width="auto" MinWidth="60" Binding="{Binding Path=KCustom}" />
                    <DataGridTextColumn Header="고객사ID" Width="auto" MinWidth="60" Binding="{Binding Path=CustomID}" />

                    <DataGridTextColumn Header="품번" Width="auto" MinWidth="60" Binding="{Binding Path=BuyerArticleNo}" />
                    <DataGridTextColumn Header="품명" Width="auto" MinWidth="60" Binding="{Binding Path=Article}" />
                    <DataGridTextColumn Header="품명코드" Width="auto" MinWidth="60" Binding="{Binding Path=ArticleID}" 
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>

                    <DataGridTextColumn Header="차종" Width="auto" MinWidth="60" Binding="{Binding Path=BuyerModelName}" />
                    <DataGridTextColumn Header="차종코드" Width="auto" MinWidth="60" Binding="{Binding Path=BuyerModelID}" />

                    <DataGridTextColumn Header="통보일" Width="auto" MinWidth="60" Binding="{Binding Path=NotifyDate}"
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="회신요청일" Width="auto" MinWidth="60" Binding="{Binding Path=ReplyReqDate}"
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="회신일" Width="auto" MinWidth="60" Binding="{Binding Path=ReplyDate}"
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="불량확인자" Width="auto" MinWidth="60" Binding="{Binding Path=DefectCheckMan}" />
                    <DataGridTextColumn Header="통보자" Width="auto" MinWidth="60" Binding="{Binding Path=NotifyMan}" />

                    <DataGridTextColumn Header="불량수량" Width="auto" MinWidth="60" Binding="{Binding Path=DefectQty}"
                                        ElementStyle="{StaticResource DataGridCellRight}"/>
                    <DataGridTextColumn Header="불량재발수량" Width="auto" MinWidth="60" Binding="{Binding Path=ReDefectQty}"
                                        ElementStyle="{StaticResource DataGridCellRight}"/>
                    <DataGridTextColumn Header="불량유형" Width="auto" MinWidth="60" Binding="{Binding Path=DefectSymtom}" />
                    <DataGridTextColumn Header="불량원인" Width="auto" MinWidth="60" Binding="{Binding Path=DefectReason}" />
                    <DataGridTextColumn Header="중요도" Width="auto" MinWidth="60" Binding="{Binding Path=ImportantGradeName}"
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>

                    <DataGridTextColumn Header="중요도ID" Width="auto" MinWidth="60" Binding="{Binding Path=ImportantGrade}"
                                        ElementStyle="{StaticResource DataGridCellRight}"/>
                    <DataGridTextColumn Header="심각도" Width="auto" MinWidth="60" Binding="{Binding Path=CriticalGradeName}" 
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="심각도ID" Width="auto" MinWidth="60" Binding="{Binding Path=CriticalGrade}" 
                                        ElementStyle="{StaticResource DataGridCellRight}"/>
                    <DataGridTextColumn Header="발생공정" Width="auto" MinWidth="60" Binding="{Binding Path=OccurProcess}" />
                    <DataGridTextColumn Header="발생호기" Width="auto" MinWidth="60" Binding="{Binding Path=OccurMachine}" />

                    <DataGridTemplateColumn Header="문제발생내역" Width="auto" MinWidth="60" >
                        <DataGridTemplateColumn.CellTemplate>
                            <DataTemplate>
                                <ScrollViewer VerticalScrollBarVisibility="Auto" Height="30">
                                    <TextBlock Text="{Binding Path=DefectContents}"  />
                                </ScrollViewer>
                            </DataTemplate>
                        </DataGridTemplateColumn.CellTemplate>
                    </DataGridTemplateColumn>

                    <DataGridTextColumn Header="접수일" Width="auto" MinWidth="60" Binding="{Binding Path=AcptDate}"
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="담당자" Width="auto" MinWidth="60" Binding="{Binding Path=AcptMan}" />
                    <DataGridTextColumn Header="검토일" Width="auto" MinWidth="60" Binding="{Binding Path=DefectRespectDate}"
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="검토자" Width="auto" MinWidth="60" Binding="{Binding Path=DefectRespectMan}" />

                    <DataGridTemplateColumn Header="개선대책" Width="auto" MinWidth="60" >
                        <DataGridTemplateColumn.CellTemplate>
                            <DataTemplate>
                                <ScrollViewer VerticalScrollBarVisibility="Auto" Height="30">
                                    <TextBlock Text="{Binding Path=DefectRespectContents}"  />
                                </ScrollViewer>
                            </DataTemplate>
                        </DataGridTemplateColumn.CellTemplate>
                    </DataGridTemplateColumn>

                    <DataGridTextColumn Header="조치예정일" Width="auto" MinWidth="60" Binding="{Binding Path=CorrExpectDate}"
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="조치완료일" Width="auto" MinWidth="60" Binding="{Binding Path=CorrCompDate}"
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="검토일(시정조치)" Width="auto" MinWidth="60" Binding="{Binding Path=CorrRespectDate}"
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="검토자(시정조치)" Width="auto" MinWidth="60" Binding="{Binding Path=CorrRespectMan}" />

                    <DataGridTextColumn Header="생산일From" Width="auto" MinWidth="60" Binding="{Binding Path=CorrProdFromDate}"
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="생산일To" Width="auto" MinWidth="60" Binding="{Binding Path=CorrProdToDate}"
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="불량유형(시정조치)" Width="auto" MinWidth="60" Binding="{Binding Path=DefectRespectSymtom}" />
                    <DataGridTextColumn Header="불량유형ID" Width="auto" MinWidth="60" Binding="{Binding Path=DefectRespectSymtomCode}" />
                    <DataGridTextColumn Header="불량원인(시정조치)" Width="auto" MinWidth="60" Binding="{Binding Path=DefectRespectReason}" />

                    <DataGridTextColumn Header="불량원인ID" Width="auto" MinWidth="60" Binding="{Binding Path=DefectRespectReasonCode}" />

                    <DataGridTemplateColumn Header="시정조치결과" Width="auto" MinWidth="60" >
                        <DataGridTemplateColumn.CellTemplate>
                            <DataTemplate>
                                <ScrollViewer VerticalScrollBarVisibility="Auto" Height="30">
                                    <TextBlock Text="{Binding Path=CorrContents}"  />
                                </ScrollViewer>
                            </DataTemplate>
                        </DataGridTemplateColumn.CellTemplate>
                    </DataGridTemplateColumn>

                    <DataGridTextColumn Header="EO변경필요여부" Width="auto" MinWidth="60" Binding="{Binding Path=CorrEOChangeYN}" 
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="4M변경대상여부" Width="auto" MinWidth="60" Binding="{Binding Path=Corr4MChangeYN}" 
                                        ElementStyle="{StaticResource DataGridCellCenter}" />
                    <DataGridTextColumn Header="설계가이드변경대상여부" Width="auto" MinWidth="60" Binding="{Binding Path=CorrDesignChangeYN}" 
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>

                    <DataGridTextColumn Header="설계가이드변경의견" Width="auto" MinWidth="60" Binding="{Binding Path=CorrDesignChangeContents}" />
                    <DataGridTextColumn Header="종결여부" Width="auto" MinWidth="60" Binding="{Binding Path=CloseYN}" 
                                        ElementStyle="{StaticResource DataGridCellCenter}"/>
                    <DataGridTextColumn Header="귀책" Width="auto" MinWidth="60" Binding="{Binding Path=ReasonImput}" />

                    <DataGridTextColumn Header="첨부화일명1" Width="auto" MinWidth="60" Binding="{Binding Path=AttFile1}" />
                    <DataGridTextColumn Header="첨부화일경로1" Width="auto" MinWidth="60" Binding="{Binding Path=AttPath1}" />
                    <DataGridTextColumn Header="첨부화일명2" Width="auto" MinWidth="60" Binding="{Binding Path=AttFile2}" />
                    <DataGridTextColumn Header="첨부화일경로2" Width="auto" MinWidth="60" Binding="{Binding Path=AttPath2}" />
                    <DataGridTextColumn Header="첨부화일명3" Width="auto" MinWidth="60" Binding="{Binding Path=AttFile3}" />
                    <DataGridTextColumn Header="첨부화일경로3" Width="auto" MinWidth="60" Binding="{Binding Path=AttPath3}" />

                </DataGrid.Columns>
            </DataGrid>
                <Grid Grid.Row="1" Grid.ColumnSpan="2">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <TextBlock x:Name="tbkIndexCount" Grid.Column="0" Text="▶검색결과 : " VerticalAlignment="Center" Margin="1"/>
                </Grid>
            </Grid>
            <!-- 그리드 스플릿터 -->
            <GridSplitter Grid.Column="1" Margin="1" Width="5" HorizontalAlignment="Stretch"/>
                

            <Grid x:Name="Sub1" Grid.Column="2" Grid.Row="1" Grid.ColumnSpan="2" >
                <Grid.RowDefinitions>
                    <RowDefinition Height="100*" />
