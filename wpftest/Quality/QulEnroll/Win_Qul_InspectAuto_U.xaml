                                    HorizontalContentAlignment="Center" VerticalContentAlignment="Center"
                                    Click="btnPfArticleName_Click">
                                <Image Source="{DynamicResource btiPlusFind}" />
                            </Button>

                            <Label x:Name="lblMachine" Grid.Column="3" Grid.Row="4" Style="{DynamicResource LabelInputArea}">
                                <TextBlock Text="호     기" />
                            </Label>
