        </Grid>

        <Grid Grid.Row="1" >
            <Grid.RowDefinitions>
                <RowDefinition Height="11*"/>
                <RowDefinition Height="9*"/>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="2*" />
                </Grid.ColumnDefinitions>
                <Grid Grid.Row="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="14*" />
                        <RowDefinition Height="1*" />
                    </Grid.RowDefinitions>
                    
                    <DataGrid x:Name="dgdMain" Grid.Row="0" SelectionChanged="dgdMain_SelectionChanged" Style="{DynamicResource DataGridSet1}">
                        <DataGrid.Columns>
                            <DataGridTemplateColumn Header="선택" >
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <CheckBox Click="chkInspect_Click">
                                            <CheckBox.Style>
                                                <Style TargetType="CheckBox">
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding Chk,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Value="true">
                                                            <Setter Property="IsChecked" Value="True"/>
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding Chk,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Value="false">
                                                            <Setter Property="IsChecked" Value="False"/>
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                                    <Setter Property="VerticalAlignment" Value="Center"/>
                                                </Style>
                                            </CheckBox.Style>
                                        </CheckBox>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>

                            <DataGridTextColumn Header="순" Binding="{Binding Num}" ElementStyle="{StaticResource DataGridCellRight}"/>
                            <DataGridTextColumn Header="InspectID" Binding="{Binding InspectID}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="검사일자" Binding="{Binding InspectDate_CV}" ElementStyle="{StaticResource DataGridCellCenter}"/>
                            <DataGridTextColumn Header="시리얼번호" Binding="{Binding SerialNo}" ElementStyle="{StaticResource DataGridCellCenter}"/>
                            <DataGridTextColumn Header="품번" Binding="{Binding BuyerArticleNo}"/>
                            <DataGridTextColumn Header="품명" Binding="{Binding Article}"/>


                            <DataGridTextColumn Header="검사구분" Binding="{Binding InspectGubun}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="LOTNO" Binding="{Binding LotID}" />
                            <DataGridTextColumn Header="검사수량" Binding="{Binding InspectQty}" ElementStyle="{StaticResource DataGridCellRight}"/>
                            <DataGridTextColumn Header="검사기준번호" Binding="{Binding InspectBasisID}" ElementStyle="{StaticResource DataGridCellCenter}"/>
                            <DataGridTextColumn Header="특이사항" Binding="{Binding Comments}"/>

                            <DataGridTextColumn Header="검사수준" Binding="{Binding InspectLevel}" ElementStyle="{StaticResource DataGridCellRight}"/>
                            <DataGridTextColumn Header="검사자" Binding="{Binding Name}"/>
                            <DataGridTextColumn Header="약도경로" Binding="{Binding SketchPath}"/>
                            <DataGridTextColumn Header="약도파일명" Binding="{Binding SketchFile}"/>
                            <DataGridTextColumn Header="파일경로" Binding="{Binding AttachedPath}"/>

                            <DataGridTextColumn Header="파일명" Binding="{Binding AttachedFile}"/>
                            <DataGridTextColumn Header="공정" Binding="{Binding Process}"/>
                            <DataGridTextColumn Header="불합격" Binding="{Binding DefectYN}" ElementStyle="{StaticResource DataGridCellCenter}"/>
                            <DataGridTextColumn Header="InspectBasisID" Binding="{Binding InspectBasisID}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="InspectBasisIDSeq" Binding="{Binding InspectBasisIDSeq}" Visibility="Hidden"/>

                            <DataGridTextColumn Header="MachineID" Binding="{Binding MachineID}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="거래처" Binding="{Binding InOutCustom}"/>
                            <DataGridTextColumn Header="거래처ID" Binding="{Binding INOutCustomID}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="입출고일자" Binding="{Binding INOutDate}"/>
                            <DataGridTextColumn Header="품질위험도" Binding="{Binding IRELevelName}"/>

                            <DataGridTextColumn Header="부품중요도 보안" Binding="{Binding ImportSecYN}" ElementStyle="{StaticResource DataGridCellCenter}"/>
                            <DataGridTextColumn Header="부품중요도 법규" Binding="{Binding ImportlawYN}" ElementStyle="{StaticResource DataGridCellCenter}"/>
                            <DataGridTextColumn Header="부품중요도 중요" Binding="{Binding ImportImpYN}" ElementStyle="{StaticResource DataGridCellCenter}"/>
                            <DataGridTextColumn Header="부품중요도 일반" Binding="{Binding ImportNorYN}" ElementStyle="{StaticResource DataGridCellCenter}"/>
                            <DataGridTextColumn Header="차종ID" Binding="{Binding BuyerModelID}"/>

                            <DataGridTextColumn Header="차종" Binding="{Binding BuyerModel}"/>
                            <DataGridTextColumn Header="초.중.종품구분" Binding="{Binding FMLGubun}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="초.중.종품구분" Binding="{Binding FMLGubunName}"/>
                            <DataGridTextColumn Header="불량제품수" Binding="{Binding TotalDefectQty}" ElementStyle="{StaticResource DataGridCellRight}"/>
                            <DataGridTextColumn Header="InspectUserID" Binding="{Binding InspectUserID}" ElementStyle="{StaticResource DataGridCellLeft}"/>
                            <DataGridTextColumn Header="MilSheetNo" Binding="{Binding MilSheetNo}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="INOUTCustomDate" Binding="{Binding INOUTCustomDate}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="기준일자" Binding="{Binding MoldNo}" Visibility="Hidden"/>
                        </DataGrid.Columns>
                    </DataGrid>

                    <TextBlock x:Name="tbkIndexCount" Grid.Row="1" Text="▶검색결과 : " VerticalAlignment="Center" HorizontalAlignment="Right" Margin="1"/>
                </Grid>

                <GridSplitter Grid.Column="1" Width="5" ResizeBehavior="PreviousAndNext"/>

                <Grid x:Name="grdInput" Grid.Column="2" IsHitTestVisible="False">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="3.1*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <GroupBox Grid.Row="0" Header="검사성적서등록">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="70*" />
                                <ColumnDefinition Width="70*" />
                                <ColumnDefinition Width="20*" />
                                <ColumnDefinition Width="70*" />
                                <ColumnDefinition Width="70*" />
                                <ColumnDefinition Width="20*" />
                                <ColumnDefinition Width="70*" />
                                <ColumnDefinition Width="70*" />
                                <ColumnDefinition Width="20*"/>
                                <ColumnDefinition Width="50*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
