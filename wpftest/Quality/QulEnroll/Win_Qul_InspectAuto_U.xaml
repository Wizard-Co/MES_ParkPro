                                    <Image Source="{DynamicResource btiPlusFind}" />
                                </Button>
                            </Grid>

                            <Label Grid.Column="0" Grid.Row="3" HorizontalContentAlignment="Center"
                                   VerticalContentAlignment="Center" Style="{DynamicResource LabelInputArea}" >
                                <TextBlock Text="차     종" />
                            </Label>
                            <TextBox x:Name="txtBuyerModel" Grid.Column="1" Grid.Row="3" Style="{DynamicResource TextBoxInputArea}"
                                     Keyboard.KeyDown="txtBuyerModel_KeyDown"
                                     Text="{Binding BuyerModel}" Tag="{Binding BuyerModelID}"/>
                            <Button x:Name="btnPfBuyerModel" Grid.Column="2" Grid.Row="3" 
                                    HorizontalContentAlignment="Center" VerticalContentAlignment="Center"
                                    Click="btnPfBuyerModel_Click">
                                <Image Source="{DynamicResource btiPlusFind}" />
                            </Button>

                            <Label x:Name="lblProcess" Grid.Column="3" Grid.Row="3" Style="{DynamicResource LabelInputArea}">
                                <TextBlock Text="공     정" />
                            </Label>
                            <ComboBox x:Name="cboProcess" Grid.Column="4" Grid.Row="3" SelectionChanged="cboProcess_SelectionChanged"
                                      Style="{DynamicResource ComboBoxInputString}"
                                      SelectedValue="{Binding ProcessID,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
                            <Label x:Name="lblInOutDate" Grid.Column="6" Grid.Row="2" Style="{DynamicResource LabelInputArea}">
                                <TextBlock x:Name="tbkInOutDate" Text="입고일"  />
