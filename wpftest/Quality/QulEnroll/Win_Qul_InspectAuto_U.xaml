                                    HorizontalContentAlignment="Center" VerticalContentAlignment="Center"
                                    Click="btnPfUser_Click">
                                <Image Source="{DynamicResource btiPlusFind}" />
                            </Button>

                            <Label Grid.Column="3" Grid.Row="7" Style="{DynamicResource LabelInputArea}">
                                <TextBlock Text="부품품질위험도"/>
                            </Label>
                            <ComboBox x:Name="cboIRELevel" Grid.Column="4" Grid.Row="7" Style="{DynamicResource ComboBoxInputString}"
                                      SelectedValue="{Binding IRELevel}"/>

                            <Label Grid.Column="0" Grid.Row="8" HorizontalContentAlignment="Center" Style="{DynamicResource LabelInputArea}" 
                                   VerticalContentAlignment="Center">
                                <TextBlock Text="특이사항" />
                            </Label>
                            <TextBox x:Name="txtComments" Grid.Column="1" Grid.Row="8" Style="{DynamicResource TextBoxInputArea}"
                                     Grid.ColumnSpan="4" Text="{Binding Comments}"/>

                            <Label Grid.Column="6" Grid.Row="3" Style="{DynamicResource LabelInputArea}" >
                                <TextBlock Text="부품중요도" />
                            </Label>
                            <Grid Grid.Column="7" Grid.Row="3" Grid.ColumnSpan="3" VerticalAlignment="Center" >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <CheckBox x:Name="chkImportSecYN" Grid.Column="0" Grid.Row="3" Style="{DynamicResource CheckBoxInputArea}">
                                    <TextBlock >보안</TextBlock>
                                </CheckBox>
                                <CheckBox x:Name="chkImportlawYN" Grid.Column="1" Grid.Row="3" Style="{DynamicResource CheckBoxInputArea}">
                                    <TextBlock >법규</TextBlock>
                                </CheckBox>
                                <CheckBox x:Name="chkImportImpYN" Grid.Column="2" Grid.Row="4" Style="{DynamicResource CheckBoxInputArea}">
                                    <TextBlock >중요</TextBlock>
                                </CheckBox>
                                <CheckBox x:Name="chkImportNorYN" Grid.Column="3" Grid.Row="4" Style="{DynamicResource CheckBoxInputArea}">
                                    <TextBlock >일반</TextBlock>
                                </CheckBox>
                            </Grid>

