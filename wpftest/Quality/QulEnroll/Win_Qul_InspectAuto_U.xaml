                            <Label Grid.Column="6" Grid.Row="5" Style="{DynamicResource LabelInputArea}">
                                <TextBlock Text="검사항목 수" />
                            </Label>
                            <Label Grid.Column="7" Grid.Row="5" Style="{DynamicResource LabelInputArea}">
                                <TextBlock Text="판정결과" />
                            </Label>
                            <Label Grid.Column="8" Grid.Row="5" Style="{DynamicResource LabelInputArea}" Grid.ColumnSpan="2">
                                <TextBlock Text="불량항목 수" />
                            </Label>

                            <TextBox x:Name="txtInspectQty" Grid.Column="6" Grid.Row="6" 
                                     Text="{Binding InspectQty}" Style="{DynamicResource TextBoxInput_Integer_Right}"/>
