                                      SelectedValue="{Binding MachineID,Mode=OneWay}"/>

                            <Label Grid.Column="0" Grid.Row="5" HorizontalContentAlignment="Center" Style="{DynamicResource LabelInputArea}" 
                                   VerticalContentAlignment="Center">
                                <TextBlock Text="품     명" />
                            </Label>
                            <TextBox x:Name="txtBuyerArticle" Grid.Column="1" Grid.Row="5" IsReadOnly="True" Style="{DynamicResource TextBoxInputArea}"
