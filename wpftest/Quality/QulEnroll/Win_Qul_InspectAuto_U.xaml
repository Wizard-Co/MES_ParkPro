                                <TextBlock Text="합계 검사수량" />
                            </Label>
                            <TextBox x:Name="txtSumInspectQty" Grid.Column="7" Grid.Row="7" Text="{Binding SumInspectQty}"  Style="{DynamicResource TextBoxInput_Integer_Right}"/>

