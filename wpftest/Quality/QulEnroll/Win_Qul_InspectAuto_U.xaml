                                <TextBlock Text="검 사 자" />
                            </Label>
                            <TextBox x:Name="txtInspectUserID" Grid.Column="1" Grid.Row="7" 
                                     Style="{DynamicResource TextBoxInputNecessary}"
                                     Keyboard.KeyDown="txtInspectUserID_KeyDown"
                                     Text="{Binding Name}" Tag="{Binding InspectUserID}"/>
