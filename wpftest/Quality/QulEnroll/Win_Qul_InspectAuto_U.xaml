                                <Image Source="{DynamicResource btiPlusFind}" />
                            </Button>

                            <Label Grid.Column="3" Grid.Row="2" Style="{DynamicResource LabelInputArea}">
                                <TextBlock Text="EO-기준-순번" />
                            </Label>
                            <ComboBox x:Name="cboEcoNO" Grid.Column="4" Grid.Row="2"
                                      Style="{DynamicResource ComboBoxInputString}"
                                      SelectionChanged="cboEcoNO_SelectionChanged"
                                      SelectedValue="{Binding InspectBasisID}"/>

                            <Label x:Name="lblInOutCustom" Grid.Column="6" Grid.Row="1" Style="{DynamicResource LabelInputArea}">
                                <TextBlock x:Name="tbkInOutCustom" Text="입고거래처" />
                            </Label>
                            <TextBox x:Name="txtInOutCustom" Grid.Column="7" Grid.Row="1" 
