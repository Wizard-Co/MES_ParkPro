                            <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}" >
                                <TextBlock Text="품    명"  Style="{DynamicResource TextBlockInViewBoxInput_Q}" />
                            </Viewbox>
                        </Label>
                        <TextBox x:Name="txtBuyerArticle" Grid.Row="1" Grid.Column="5"  IsReadOnly="True" Style="{DynamicResource TextBoxInputArea_Q}"
                                 Text="{Binding Article}"/>


