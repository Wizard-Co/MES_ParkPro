                                <DataTemplate>
                                    <TextBlock Margin="2" Text="{Binding ManageGubunname}" Tag="{Binding ManageGubun}"/>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                            <DataGridTemplateColumn.CellEditingTemplate>
                                <DataTemplate>
                                    <ComboBox Text="{Binding ManageGubunname,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" 
                                              SelectedValue="{Binding ManageGubun,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"
                                              ItemsSource="{Binding ovcManage}" DisplayMemberPath="code_name" SelectedValuePath="code_id"
                                              />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellEditingTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn Header="주기구분">
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Margin="2" Text="{Binding InspectCycleGubunName}" Tag="{Binding InspectCycleGubun}"/>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                            <DataGridTemplateColumn.CellEditingTemplate>
                                <DataTemplate>
                                    <ComboBox Text="{Binding InspectCycleGubunName,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" 
