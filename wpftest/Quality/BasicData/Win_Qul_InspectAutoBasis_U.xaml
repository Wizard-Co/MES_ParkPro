                            <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}" >
                                <TextBlock Text="기준번호"  Style="{DynamicResource TextBlockInViewBoxInput_Four_Q}" />
                            </Viewbox>
                        </Label>
                        <TextBox x:Name="txtInspectBasisID" Grid.Row="1" Grid.Column="1" IsReadOnly="True"
                                 Text="{Binding InspectBasisID}" Style="{DynamicResource TextBoxInputAutoCreate}"/>

                        <Label Grid.Row="2" Grid.Column="4" Style="{DynamicResource LabelInputArea_GB_Q}">
                            <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}" >
                                <TextBlock Text="EO NO"  Style="{DynamicResource TextBlockInViewBoxInput_Q}" />
                            </Viewbox>
                        </Label>
                        <TextBox x:Name="txtECONO" Grid.Row="2" Grid.Column="5"  Style="{DynamicResource TextBoxInputArea_Q}"
