                    <ToggleButton x:Name="tbnProcessCycle" Grid.Column="0" Click="TbnProcessCycle_Click" FontWeight="Normal">
                        <StackPanel Orientation="Horizontal">
                            <Viewbox Margin="0.5">
                                <TextBlock Text="공정순회" />
                            </Viewbox>
                        </StackPanel>
                        <ToggleButton.Style>
                            <Style TargetType="{x:Type ToggleButton}">
