                                              ItemsSource="{Binding ovcCycle}" DisplayMemberPath="code_name" SelectedValuePath="code_id"/>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellEditingTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTextColumn Header="주기" Binding="{Binding InspectCycle,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" ElementStyle="{DynamicResource DataGridCellRight}"/>
                        <DataGridTemplateColumn Header="이미지" >
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding InsImageFile, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" Margin="2" />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                            <DataGridTemplateColumn.CellEditingTemplate>
                                <DataTemplate>
                                    <TextBox Text="{Binding InsImageFile, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" KeyDown="dgdtpetxtImage_KeyDown"/>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellEditingTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn Header="보기">
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <!--<TextBlock Margin="2" Text="{Binding McImageView}" />-->
                                    <Button Click="btnSeeImage_Click" >
                                        <Button.Style>
                                            <Style TargetType="Button">
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding imageFlag}" Value="true">
                                                        <Setter Property="IsEnabled" Value="True" />
                                                        <Setter Property="Background" Value="#eff9ff" />
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding imageFlag}" Value="false">
                                                        <Setter Property="IsEnabled" Value="False" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Button.Style>
                                        <StackPanel Orientation="Horizontal">
                                            <Image Source="{Binding ImageView}" Width="100" Height="30">
                                                <Image.Style>
                                                    <Style TargetType="Image">
                                                        <Style.Triggers>
                                                            <DataTrigger Binding="{Binding imageFlag}" Value="true">
                                                                <Setter Property="Visibility" Value="Visible" />
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{Binding imageFlag}" Value="false">
                                                                <Setter Property="Visibility" Value="Collapsed" />
                                                            </DataTrigger>
                                                        </Style.Triggers>
                                                    </Style>
                                                </Image.Style>
                                            </Image>
                                            <TextBlock Text="보 기" HorizontalAlignment="Center">
                                                <TextBlock.Style>
                                                    <Style TargetType="TextBlock">
                                                        <Style.Triggers>
                                                            <DataTrigger Binding="{Binding imageFlag}" Value="true" >
                                                                <Setter Property="Visibility" Value="Collapsed" />
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{Binding imageFlag}" Value="false" >
                                                                <Setter Property="Visibility" Value="Visible" />
                                                            </DataTrigger>
                                                        </Style.Triggers>
                                                    </Style>
                                                </TextBlock.Style>
                                            </TextBlock>
                                        </StackPanel>
                                    </Button>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>

                        <DataGridTextColumn Header="비고" Binding="{Binding Comments
                            ,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"/>
                    </DataGrid.Columns>
                </DataGrid>
                <Grid Grid.Row="3">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Viewbox Grid.Row="0" HorizontalAlignment="Left" Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}">
                        <TextBlock Text="☞ 관리구분 : S-특별특성항목, D-일상검사항목, P-정기검사항목 ex) SP-특별특성_정기검사항목 입니다." Foreground="Blue" Style="{DynamicResource TextBlockInViewBoxOneLine_Q}"/>
                    </Viewbox>
                </Grid>
            </Grid>
        </Grid>
    </Grid>
