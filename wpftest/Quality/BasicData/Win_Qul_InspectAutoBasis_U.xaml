                    <Button Grid.Column="3"  x:Name="subAdd" IsEnabled="False" Click="SubAdd_Click" >
                        <TextBlock Text="추가" />
                    </Button>
                    <Button Grid.Column="4" x:Name="subDel" IsEnabled="False" Click="SubDel_Click" >
                        <TextBlock Text="삭제" />
                    </Button>
                </Grid>
                <DataGrid x:Name="dgdSub" Grid.Row="2" Style="{DynamicResource DataGridSet1}" >
                    <DataGrid.Resources>
                        <Style TargetType="DataGridCell">
                            <EventSetter Event="PreviewKeyDown" Handler="DataGridSubCell_PreviewKeyDown"/>
                            <EventSetter Event="KeyDown" Handler="DataGridSubCell_KeyDown" />
                            <EventSetter Event="KeyUp" Handler="TextBoxFocusInDataGrid" />
                            <EventSetter Event="MouseUp" Handler="TextBoxFocusInDataGrid_MouseUp" />
                            <EventSetter Event="GotFocus" Handler="DataGridCell_GotFocus" />
                        </Style>
                    </DataGrid.Resources>
                    <DataGrid.Columns>
                        <DataGridTemplateColumn Header="">
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Margin="2" Text="{Binding Num}" HorizontalAlignment="Right"/>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn Header="외관/DIM's">
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Margin="2" Text="{Binding insTypeText}" Tag="{Binding insType}"/>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                            <DataGridTemplateColumn.CellEditingTemplate>
                                <DataTemplate>
                                    <ComboBox SelectedValue="{Binding insType,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" 
                                              Text="{Binding insTypeText,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" 
                                              ItemsSource="{Binding ovcType}" DisplayMemberPath="code_name" SelectedValuePath="code_id"/>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellEditingTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTextColumn Header="검사항목명" Binding="{Binding insItemName
                            ,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"/>
                        <DataGridTextColumn Header="검사기준 규격" Binding="{Binding Spec
                            ,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"/>
                        <DataGridTemplateColumn Header="검사기준 하한">
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Margin="2" Text="{Binding SpecMin,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"/>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                            <DataGridTemplateColumn.CellEditingTemplate>
                                <DataTemplate>
                                    <TextBox Text="{Binding SpecMin,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"
                                             Tag ="{Binding SpecMin,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellEditingTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn Header="검사기준 상한">
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Margin="2" Text="{Binding SpecMax,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"/>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                            <DataGridTemplateColumn.CellEditingTemplate>
                                <DataTemplate>
                                    <TextBox Text="{Binding SpecMax,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"
                                             Tag ="{Binding SpecMax,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellEditingTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTextColumn Header="샘플수량" Binding="{Binding InsSampleQty
                            ,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"/>
                        
                        <DataGridTemplateColumn x:Name="dgdtpeInspectGage" Header="측정Gage">
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Margin="2" Text="{Binding InspectGageName,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" 
                                                   Tag="{Binding InspectGage,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                            <DataGridTemplateColumn.CellEditingTemplate>
                                <DataTemplate>
                                    <TextBox Text="{Binding InspectGageName,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" 
