                                 Text="{Binding MoldNo}" Keyboard.KeyDown="txtMoldNo_KeyDown"/>-->
                        <DatePicker  Grid.Row="3" Grid.Column="5"  x:Name="dtpMoldNo" Padding="0" Style="{DynamicResource DatePickerInputNecessary_Q}"
