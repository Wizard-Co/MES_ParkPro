                            <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight_Q}" >
                                <TextBlock Text="품    번"  Style="{DynamicResource TextBlockInViewBoxInput_Q}" />
                            </Viewbox>
                        </Label>
