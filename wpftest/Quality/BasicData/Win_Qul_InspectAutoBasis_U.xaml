                    <ColumnDefinition Width="75*"/>
                    <ColumnDefinition Width="90*"/>
                    <ColumnDefinition Width="15*"/>
                </Grid.ColumnDefinitions>
                <Label x:Name="lblECONOSrh" Grid.Column="0" VerticalContentAlignment="Center" Style="{DynamicResource LabelSearch_Q}"
