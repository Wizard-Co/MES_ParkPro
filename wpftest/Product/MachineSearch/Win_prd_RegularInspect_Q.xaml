                            <DataGridTextColumn Header="비 고" Width="*" Binding="{Binding Comments}"/>
                            <DataGridTextColumn Header="McRInspectID" Width="*" Binding="{Binding McRInspectID}" Visibility="Hidden"/>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
