                            <DataGridTextColumn Header="설비명" Width="*" Binding="{Binding McName}"/>
                            <DataGridTextColumn Header="정기검사구분" Width="*" Binding="{Binding McInsCycle}"/>
                            <DataGridTextColumn Header="개정일자" Width="*" Binding="{Binding McInsBasisDate}"/>
                            <DataGridTextColumn Header="검사일자" Width="*" Binding="{Binding McRInspectDate}"/>
                            <DataGridTextColumn Header="검사자" Width="*" Binding="{Binding Name}"/>
                            <DataGridTextColumn Header="검사번호" Width="auto" Binding="{Binding McRInspectID}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="검사자ID" Width="auto" Binding="{Binding McRInspectUserID}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="MCID" Width="auto" Binding="{Binding MCID}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="검사주기구분" Width="auto" Binding="{Binding McInsCycleGbn}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="문제내역" Width="auto" Binding="{Binding DefectContents}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="문제원인" Width="auto" Binding="{Binding DefectReason}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="조치및대책" Width="auto" Binding="{Binding DefectRespectContents}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="비고" Width="*" Binding="{Binding Comments}"/>
                            <DataGridTextColumn Header="검사기준ID" Width="auto" Binding="{Binding McInspectBasisID}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="ManagerID" Width="auto" Binding="{Binding ManagerID}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="BuyDate" Width="auto" Binding="{Binding BuyDate}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="ImageFile" Width="auto" Binding="{Binding ImageFile}" Visibility="Hidden"/>
                            <DataGridTextColumn Header="ImagePath" Width="auto" Binding="{Binding ImagePath}" Visibility="Hidden"/>
                        </DataGrid.Columns>
                    </DataGrid>
                    <GridSplitter Grid.Column="1" Width="5" ResizeBehavior="PreviousAndNext"/>
                    <DataGrid x:Name="dgdMCPartRInsTwo" Grid.Column="2" Style="{DynamicResource DataGridSet1}" >
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="날 짜" Width="*" Binding="{Binding McRInspectDate}"/>
