                                        </Viewbox>
                                    </Label>
                                    <TextBox x:Name="txtOutCustom" Grid.Column="1" Grid.ColumnSpan="3" Grid.Row="3" Background="White"  Style="{DynamicResource TextBoxInputArea}" 
