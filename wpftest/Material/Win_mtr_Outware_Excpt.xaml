                                    <Label x:Name="lblCustom" Grid.Column="0" Grid.Row="2"  Style="{DynamicResource LabelInputArea}">
                                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                                            <TextBlock Text="거래처" Style="{DynamicResource TextBlockInViewBoxInput}"/>
                                        </Viewbox>
                                    </Label>
                                    <TextBox x:Name="txtCustom" Grid.Column="1" Grid.ColumnSpan="3" Grid.Row="2"  Style="{DynamicResource TextBoxInputNecessary}" 
                                         Text="{Binding CustomName}" Tag="{Binding OutCustomID}" Keyboard.KeyDown="txtCustom_KeyDown" />
                                    <Button x:Name="btnCustom" Grid.Column="4" Grid.Row="2"  Click="btnCustom_Click" >
