                                    <!--출고처-->
                                    <Label x:Name="lblDvlyCustom" Grid.Column="0" Grid.Row="5"  Style="{DynamicResource LabelInputArea}">
                                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                                            <TextBlock Text="납품장소" Style="{DynamicResource TextBlockInViewBoxInput}"/>
                                        </Viewbox>
                                    </Label>
                                    <TextBox x:Name="txtDvlyCustom" Grid.Column="1" Grid.ColumnSpan="3" Grid.Row="5" Background="White"  Style="{DynamicResource TextBoxInputArea}" 
                                         Text="{Binding DvlyCustom}" Keyboard.KeyDown="TxtDvlyCustom_KeyDown" />
                                    <Button x:Name="btnDvlyCustom" Grid.Column="3" Grid.Row="5"  Click="BtnDvlyCustom_Click" Visibility="Hidden">
