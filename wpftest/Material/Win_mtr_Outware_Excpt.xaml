                                    </Button>

                                    <!-- 작성일자 / 단위 -->
                                    <Label x:Name="lblOutDate" Grid.Column="0" Grid.Row="0" Style="{DynamicResource LabelInputArea}">
                                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                                            <TextBlock Text="작성일자" Style="{DynamicResource TextBlockInViewBoxInput}"/>
                                        </Viewbox>
                                    </Label>
                                    <DatePicker x:Name="dtpOutDate" Grid.Column="1" Grid.ColumnSpan="3" Grid.Row="0" Style="{DynamicResource DatePickerInputArea}"
                                            SelectedDate="{Binding OutDate_CV}"/>

                                    <Label x:Name="lblUnitClss" Grid.Column="8" Grid.Row="1"  Style="{DynamicResource LabelInputArea}" Visibility="Hidden">
                                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                                            <TextBlock Text="단  위" Style="{DynamicResource TextBlockInViewBoxInput}" />
                                        </Viewbox>
                                    </Label>
                                    <ComboBox x:Name="cboUnitClss" Grid.Column="9" Grid.ColumnSpan="2" Grid.Row="1" Background="#fff2d2" Style="{DynamicResource ComboBoxInputString}"
                                          SelectedValue="{Binding UnitClss}"
                                              Visibility="Hidden"/>

                                    <!-- 수량(박스수 / 수량) / 이동번호 -->
                                    <Label x:Name="lblOutQty" Grid.Column="0" Grid.Row="7"  Style="{DynamicResource LabelInputArea}">
                                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                                            <TextBlock Text="박스수 / 총수량" Style="{DynamicResource TextBlockInViewBoxInput}"/>
                                        </Viewbox>
                                    </Label>
                                    <TextBox x:Name="txtOutRoll" Grid.Column="1" Grid.Row="7" Background="#fff2d2" Style="{DynamicResource TextBoxInput_Number_Right}"
                                         Text="{Binding OutRoll}" IsHitTestVisible="False"/>
                                    <TextBox x:Name="txtOutQty" Grid.Column="2" Grid.ColumnSpan="2" Grid.Row="7" Background="#fff2d2" Style="{DynamicResource TextBoxInput_Number_Right}"
                                         Text="{Binding OutQty}" IsHitTestVisible="False"/>

                                    <Label x:Name="lblOutwareID" Grid.Column="5" Grid.Row="5"  Style="{DynamicResource LabelInputArea}">
                                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                                            <TextBlock Text="출고번호"  Style="{DynamicResource TextBlockInViewBoxInput}"/>
                                        </Viewbox>
                                    </Label>
                                    <TextBox x:Name="cboOutwareID" Grid.Column="6" Grid.ColumnSpan="4" Grid.Row="5"  Style="{DynamicResource TextBoxInputAutoCreate}" IsReadOnly="True"
                                         Text="{Binding OutwareID}"/>

                                    <!-- 비고 및 체크박스 -->
                                    <Label x:Name="lblRemark" Grid.Column="0" Grid.Row="6" Style="{DynamicResource LabelInputArea}">
                                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                                            <TextBlock  Text="비   고" Style="{DynamicResource TextBlockInViewBoxInput}"/>
                                        </Viewbox>
                                    </Label>
                                    <TextBox x:Name="txtRemark" Grid.Column="1" Grid.ColumnSpan="7" Grid.Row="6" Background="White"  Style="{DynamicResource TextBoxInputArea}" 
                                         Text="{Binding Remark}"/>



                                    <Label x:Name="lblAmount" Grid.Column="0" Grid.Row="8"  Style="{DynamicResource LabelInputArea}">
                                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                                            <TextBlock Text="금  액" Style="{DynamicResource TextBlockInViewBoxInput}"/>
                                        </Viewbox>
                                    </Label>
                                    <TextBox x:Name="txtAmount" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="8" Background="#fff2d2" Style="{DynamicResource TextBoxInput_Number_Right}"
                                         Text="{Binding Amount}" IsHitTestVisible="False"/>


                                    <Label Grid.Column="8" Grid.ColumnSpan="2" Grid.Row="8"  Style="{DynamicResource LabelInputArea}">
                                        <StackPanel Style="{DynamicResource StackPanelInLabel}">

                                            <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                                                <CheckBox x:Name="chkBuyerDirectYN"  Background="#fff2d2" Style="{DynamicResource CheckBoxInputArea}"  />
                                            </Viewbox>
                                            <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                                                <TextBlock  Text="고객 직접출고" Style="{DynamicResource TextBlockInViewBoxInput}"/>
                                            </Viewbox>


                                        </StackPanel>

                                    </Label>
                                </Grid>
                            </GroupBox>


                        </Grid>

                    </Grid>

                    <Grid  Grid.Row="1">
                        <Border Grid.Row="1"  BorderBrush="Black" BorderThickness="2" Margin="3 1" Background="#80ff80" MouseLeftButtonUp="borderBarcode_MouseLeftButtonUp">
                            <Viewbox HorizontalAlignment="Left">
                                <TextBox x:Name="txtBarCode" KeyDown="txtBarCode_KeyDown" KeyUp="txtBarCode_KeyUp" Margin="1" IsHitTestVisible="False" VerticalContentAlignment="Center" HorizontalContentAlignment="Left"  Background="#80ff80"/>
                            </Viewbox>
                        </Border>
                    </Grid>
                    <!-- 서브 그리드 -->
                    <Grid Grid.Row="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="8.5*" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="10*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Button x:Name="btnAddSub" Grid.Column="1" Content="추가" Margin="1 1 3 1" Click="btnAddSub_Click" IsEnabled="False"/>
                            <Button x:Name="btnDeleteSub" Grid.Column="2" Content="삭제" Margin="3 1 1 1" Click="btnDeleteSub_Click" IsEnabled="False"/>
                        </Grid>
                        <DataGrid Grid.Row="0" x:Name="dgdSub" ColumnHeaderHeight="35" Visibility="Visible" Style="{StaticResource DataGridSet1}" >
                            <DataGrid.CellStyle>
                                <Style TargetType="DataGridCell">
                                    <EventSetter Event="PreviewKeyDown" Handler="DataGridSub_PreviewKeyDown"/>
                                    <EventSetter Event="KeyDown" Handler="DataGridSub_KeyDown"/>
                                    <EventSetter Event="KeyUp" Handler="DataGridSub_TextFocus"/>
                                    <EventSetter Event="GotFocus" Handler="DataGridSub_GotFocus" />
                                    <EventSetter Event="MouseUp" Handler="DataGridSub_MouseUp" />
                                </Style>
                            </DataGrid.CellStyle>

                            <DataGrid.RowHeaderStyle>
                                <Style TargetType="DataGridRowHeader" >
                                    <Setter Property="MinHeight" Value="30" />
                                </Style>
                            </DataGrid.RowHeaderStyle>
                            <DataGrid.Columns>
                                <DataGridTemplateColumn Header="">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Num}" Margin="1" FontSize="20" TextAlignment="Center"/>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="바코드번호" Width="auto" MinWidth="100">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding LabelID}" Margin="1" FontSize="20" TextAlignment="Left" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="바코드구분" Width="auto" MinWidth="100">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding LabelGubunName}" Margin="1" FontSize="20" TextAlignment="Center"/>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="수량" Width="auto" MinWidth="100">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Qty,StringFormat=N0}" Margin="1" FontSize="20"
                                                   TextAlignment="Right" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="단위" Width="auto" MinWidth="100">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding UnitClssName}" Margin="1" FontSize="20"
                                                   TextAlignment="Right" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>

                                <DataGridTemplateColumn Header="단가" Width="auto" MinWidth="150" x:Name="GridAmount">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding UnitPrice, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"  Foreground="Black" FontSize="20"
                                                           TextAlignment="Center" VerticalAlignment="Center"/>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                    <DataGridTemplateColumn.CellEditingTemplate>
                                        <DataTemplate>
                                            <TextBox Text="{Binding UnitPrice, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" 
                                                      Style="{DynamicResource TextBoxInput_NumberCommaZero_Right}"
                                                     x:Name="GtxtPrice"
                                                     TextAlignment="Center" Foreground="Black" FontSize="20" 
                                                     TextChanged="TextBox_TextChanged" 
                                                     PreviewKeyDown="TextBox_PreviewKeyDown"/>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellEditingTemplate>
                                </DataGridTemplateColumn>


                                <DataGridTemplateColumn Header="비고" Width="auto" MinWidth="150" >
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding SubRemark, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"  Foreground="Black" FontSize="20"
                                                           TextAlignment="Center" VerticalAlignment="Center"/>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                    <DataGridTemplateColumn.CellEditingTemplate>
                                        <DataTemplate>
                                            <TextBox Text="{Binding SubRemark, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" TextAlignment="Center" Foreground="Black" FontSize="20"/>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellEditingTemplate>
                                </DataGridTemplateColumn>

                            </DataGrid.Columns>
                        </DataGrid>

                        <!-- 부분처리 그리드 -->
                        <Grid x:Name="gridPart" Visibility="Visible" Grid.Row="0">
                            <!--<Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="5.5*" />
                            </Grid.RowDefinitions>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{Binding ElementName=UtilMM, Path=ActualWidth}" />
                                    <ColumnDefinition Width="{Binding ElementName=WaterUseQty, Path=ActualWidth}" />
                                    <ColumnDefinition Width="{Binding ElementName=WaterUseAmount, Path=ActualWidth}" />
                                    <ColumnDefinition Width="{Binding ElementName=GasUseQty, Path=ActualWidth}" />
                                    <ColumnDefinition Width="{Binding ElementName=GasUseAmount, Path=ActualWidth}" />
                                    <ColumnDefinition Width="{Binding ElementName=ElecUseQty, Path=ActualWidth}" />
                                    <ColumnDefinition Width="{Binding ElementName=ElecUseAmount, Path=ActualWidth}" />
                                </Grid.ColumnDefinitions>
                                <DataGridColumnHeader Grid.Row="0" Grid.RowSpan="2" Grid.Column="0" Content="  " />
                                <DataGridColumnHeader Grid.Row="0" Grid.RowSpan="2" Grid.Column="1" Content="라벨" />
                                <DataGridColumnHeader Grid.Row="0" Grid.RowSpan="2" Grid.Column="2">
                                    <Viewbox>
                                        <StackPanel>
                                            <TextBlock Text="입고" />
                                            <TextBlock Text="수량" />
                                        </StackPanel>
                                    </Viewbox>
                                </DataGridColumnHeader>
                                <DataGridColumnHeader Grid.Row="0" Grid.RowSpan="2" Grid.Column="3" >
                                    <Viewbox>
                                        <StackPanel>
                                            <TextBlock Text="정상" />
                                            <TextBlock Text="수량" />
                                        </StackPanel>
                                    </Viewbox>
                                </DataGridColumnHeader>
                                <DataGridColumnHeader Grid.Row="0" Grid.RowSpan="2" Grid.Column="4" >
                                    <Viewbox>
                                        <StackPanel>
                                            <TextBlock Text="샘플" />
                                            <TextBlock Text="수량" />
                                        </StackPanel>
                                    </Viewbox>
                                </DataGridColumnHeader>
                                <DataGridColumnHeader Grid.Row="0" Grid.Column="5" Grid.ColumnSpan="2">
                                    <Viewbox>
                                        <TextBlock Text="불량"/>
                                    </Viewbox>
                                </DataGridColumnHeader>
                                <DataGridColumnHeader Grid.Row="1" Grid.Column="5">
                                    <Viewbox>
                                        <TextBlock Text="불량코드"/>
                                    </Viewbox>
                                </DataGridColumnHeader>
                                <DataGridColumnHeader Grid.Row="1" Grid.Column="6">
                                    <Viewbox>
                                        <TextBlock Text="수량"/>
                                    </Viewbox>
                                </DataGridColumnHeader>
                            </Grid>-->
                            <DataGrid Grid.Row="1" x:Name="dgdPart" Visibility="Hidden" ColumnHeaderHeight="35">
                                <DataGrid.CellStyle>
                                    <Style TargetType="DataGridCell">
                                        <EventSetter Event="PreviewKeyDown" Handler="DataGird_PreviewKeyDown"/>
                                        <EventSetter Event="KeyDown" Handler="DataGird_KeyDown"/>
                                        <EventSetter Event="KeyUp" Handler="DatagridIn_TextFocus"/>
                                        <EventSetter Event="GotFocus" Handler="DataGridCell_GotFocus" />
                                        <EventSetter Event="MouseUp" Handler="DataGridCell_MouseUp" />
                                        <!--<EventSetter Event="LostFocus" Handler="DataGridCell_LostFocus" />-->
                                    </Style>
                                </DataGrid.CellStyle>
                                <DataGrid.RowHeaderStyle>
                                    <Style TargetType="DataGridRowHeader" >
                                        <Setter Property="MinHeight" Value="30" />
                                    </Style>
                                </DataGrid.RowHeaderStyle>
                                <DataGrid.Columns>
                                    <DataGridTemplateColumn Header=" " Width="auto">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding Num}" VerticalAlignment="Center" TextAlignment="Center"/>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>
                                    <DataGridTemplateColumn Header="라벨" Width="2*">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding LabelID, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" VerticalAlignment="Center" TextAlignment="Center"/>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                        <!--<DataGridTemplateColumn.CellEditingTemplate>
                                            <DataTemplate>
                                                <TextBox Text="{Binding LabelID, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" VerticalAlignment="Center" TextAlignment="Center" />
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellEditingTemplate>-->
                                    </DataGridTemplateColumn>
                                    <DataGridTemplateColumn Header="합계수량" Width="*">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding Qty, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" VerticalAlignment="Center" TextAlignment="Right" Padding="0 0 7 0"/>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                        <!--<DataGridTemplateColumn.CellEditingTemplate>
                                            <DataTemplate>
                                                <TextBox Text="{Binding Qty}" VerticalAlignment="Center" TextAlignment="Center" />
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellEditingTemplate>-->
                                    </DataGridTemplateColumn>
                                    <DataGridTemplateColumn Header="정상수량" Width="*">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding NQty, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" VerticalAlignment="Center" TextAlignment="Right" Padding="0 0 7 0"/>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                        <DataGridTemplateColumn.CellEditingTemplate>
                                            <DataTemplate>
                                                <TextBox Text="{Binding NQty, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" VerticalAlignment="Center" TextAlignment="Center" />
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellEditingTemplate>
                                    </DataGridTemplateColumn>
                                    <DataGridTemplateColumn Header="샘플수량" Width="*">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding SQty, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" VerticalAlignment="Center" TextAlignment="Right" Padding="0 0 7 0"/>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                        <DataGridTemplateColumn.CellEditingTemplate>
                                            <DataTemplate>
                                                <TextBox Text="{Binding SQty, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" VerticalAlignment="Center" TextAlignment="Center" />
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellEditingTemplate>
                                    </DataGridTemplateColumn>
                                    <DataGridTemplateColumn Header="불량코드" Width="*">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding DefectName, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" 
                                                           Tag="{Binding DefectID, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" VerticalAlignment="Center" TextAlignment="Center" />
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                        <DataGridTemplateColumn.CellEditingTemplate>
                                            <DataTemplate>
                                                <TextBox Text="{Binding DefectName, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" 
                                                         Tag="{Binding DefectID, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" 
                                                         VerticalAlignment="Center" TextAlignment="Center" KeyDown="txtDefect_KeyDown"/>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellEditingTemplate>
                                    </DataGridTemplateColumn>
                                    <DataGridTemplateColumn Header="불량수량" Width="*">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding DQty, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" VerticalAlignment="Center" TextAlignment="Right" Padding="0 0 7 0"/>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                        <DataGridTemplateColumn.CellEditingTemplate>
                                            <DataTemplate>
                                                <TextBox x:Name="txtDgdDefectQty" Text="{Binding DQty, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" VerticalAlignment="Center" TextAlignment="Center" />
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellEditingTemplate>
                                    </DataGridTemplateColumn>
                                </DataGrid.Columns>
                            </DataGrid>
                        </Grid>
                    </Grid>
                </Grid>
                <Grid Grid.Row="2" Grid.ColumnSpan="3">
                    <Border BorderBrush="Gray" BorderThickness="1" Margin="1">
                        <Viewbox HorizontalAlignment="Left" VerticalAlignment="Top">
                            <StackPanel Orientation="Vertical">
                                <TextBlock Margin="1" Foreground="Blue" 
                                   Text="☞ ID기준 작업시 처리할 로트 또는 박스를 초록색 박스에서 스캔하고, 수량 기준 건은 초록색 박스에 수량을 입력 후 엔터키를 치세요." />
                                <TextBlock Margin="1" Foreground="Blue" 
                                   Text="☞ 수량 기준 건은 초록색 박스에 수량을 입력 후 엔터키를 치세요." />
                                <TextBlock Margin="1" Foreground="Blue" 
                                   Text="☞ 삭제시에는 바코드번호란에서 '삭제버튼'을 누르시면 됩니다." />
                            </StackPanel>
                        </Viewbox>
                    </Border>
                </Grid>
            </Grid>
        </Grid>
    </Grid>
</UserControl>
